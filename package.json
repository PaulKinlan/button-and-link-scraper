{
  "name": "button-and-link-scraper",
  "version": "1.0.0",
  "description": "Scrapes buttons and links to help train ML",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./data/images/{buttons,text-links,image-links}/*.png",
    "generate-zip": "(cd ./data && tar -cvzf images.tgz ./images/)",
    "sha": "find ./data/images -name '*.png' -type f | xargs -I {} shasum {} | sort > ./data/images.sha",
    "dedupe-images": "find ./data/images/buttons -name '*.png' -type f | xargs -I {} shasum {} | awk 'h[$1]{ printf \"%s \", $2; next }{ h[$1] = $2 }' | xargs rm",
    "scrape": "cat urls.txt | xargs | sed 's/ / --url=/g' | xargs node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulKinlan/button-and-link-scraper.git"
  },
  "author": "Paul Kinlan",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/PaulKinlan/button-and-link-scraper/issues"
  },
  "type": "module",
  "homepage": "https://github.com/PaulKinlan/button-and-link-scraper#readme",
  "dependencies": {
    "minimist": "^1.2.7",
    "puppeteer": "^19.4.0",
    "uuid": "^9.0.0"
  }
}
